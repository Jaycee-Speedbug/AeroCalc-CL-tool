<?xml version="1.0" encoding="UTF-8"?>

<Application name="AeroCalcCore">
    <Authors>
        <Author>JCL</Author>
    </Authors>
    <Configuration version="1.0">
        <Directories>
            <Directory name="Models">models</Directory>
            <Directory name="Scripts">scripts</Directory>
            <Directory name="Configuration">config</Directory>
            <Directory name="Logs">logs</Directory>
        </Directories>
        <Files>
            <!--File name="Units">UnitsDictionary.csv</File-->
            <File name="Units">units.xml</File>
        </Files>
        <Languages>
            <Language shortname="FR" name="french">fr.xml</Language>
            <Language shortname="EN" name="english">en.xml</Language>
        </Languages>
    </Configuration>
    <Settings>
        <Setting name="VerboseAllowed">true</Setting>
        <Setting name="Verbose">false</Setting>
        <Setting name="UnitsEnabled">true</Setting>
        <Setting name="PublicVersion">DEV</Setting>
        <Setting name="PublicAppName">AeroCalc Command Line</Setting>
        <Setting name="Logger">true</Setting>
        <Setting name="Language">FR</Setting>
    </Settings>
    

</Application>

